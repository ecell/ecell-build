MAKE_INSTALL_TARGET on libjpeg = install-lib ;
BuildAutoconf libjpeg    : jpeg-6b
                         : --enable-shared ;
Unpack jpeg-6b           : $(ARCHIVEDIR)/jpegsrc.v6b.tar.gz
                         : DESTDIR.patch ;

Download $(ARCHIVEDIR)/jpegsrc.v6b.tar.gz :
    http://www.ijg.org/files/jpegsrc.v6b.tar.gz ;
